<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Contact_must_be_in_Account_ZIP_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( NOT(ISBLANK( AccountId )) ,
 MailingPostalCode  &lt;&gt;  Account.ShippingPostalCode )</errorConditionFormula>
        <errorMessage>Mailing Code can&apos;t be different from Shipping Code</errorMessage>
    </validationRules>
</CustomObject>
